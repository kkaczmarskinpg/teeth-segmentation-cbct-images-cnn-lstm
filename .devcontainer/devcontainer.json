{
    "name": "kkaczmarski-tf-gpu",
    "dockerComposeFile": [],
    "service": "kkaczmarski-tf-gpu",
    "workspaceFolder": "/ws",
    "settings": {
      "python.pythonPath": "/opt/conda/bin/python",
      "terminal.integrated.defaultProfile.linux": "bash"
    },
    "extensions": [
      "ms-python.python",
      "ms-toolsai.jupyter",
      "ms-azuretools.vscode-docker"
    ],
    "postCreateCommand": "conda init bash && source ~/.bashrc && conda activate base",
    "remoteUser": "root"
  }
  